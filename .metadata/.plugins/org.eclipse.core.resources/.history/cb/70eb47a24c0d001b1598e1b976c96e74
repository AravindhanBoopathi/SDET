package ds.arrays.string.assessment;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.LinkedList;
import java.util.List;
import java.util.Map;
import java.util.Map.Entry;

import org.junit.Test;

public class P007HowManyWords {

	
	private int solution(String input){

		int index = 0;
		
		Map<Character, Integer> map = new HashMap<Character, Integer>();

		for (char c : input.toCharArray()) {
			map.put(c, map.getOrDefault(c, 0) + 1);
		}

		
		List<Entry<Character,Integer>> output1 = new LinkedList<Entry<Character,Integer>>(map.entrySet());
		
		List<Character> output = new ArrayList<Character>();

		for(char i: input.toCharArray()){
			output.add(i);
		}
		
		System.out.println(output1);
		System.out.println(output);
		
		char c1 = output1.get(0).getKey();
		char c2 = output.get(0);
		
		if(c1 == c2){
			System.out.println("bozo");
		}
		
		return index;
	}

	@Test
	public void testData1(){

		String input = "abc";

		System.out.println(solution(input));

	}
	
}
